# Traefik Configuration
# Copy this file to .env and update with your settings

# Let's Encrypt Email
ACME_EMAIL=admin@mucks.me

# Traefik Dashboard Domain
TRAEFIK_DOMAIN=traefik.mucks.me

# Traefik Dashboard Basic Auth
# Generate with: docker run --rm httpd:alpine htpasswd -nb admin yourpassword
# Example below is admin:admin (change this!)
TRAEFIK_BASIC_AUTH=admin:$$apr1$$8EVjn/nj$$GiLUZqcbueTFeD23SuB6x0

# Log Level (DEBUG, INFO, WARN, ERROR)
LOG_LEVEL=INFO

# Optional: Cloudflare API Token for DNS challenge
# CF_DNS_API_TOKEN=your_cloudflare_token_here

